{
  "cli": {
    "version": ">= 9.1.0",
    "requireCommit": true
  },
  "build": {
    "base": {
      "node": "18.18.2"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "testflight-android": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "autoIncrement": true
      },
      "android": {
        "autoIncrement": true
      },
      "channel": "testflight",
      "env": {
        "EXPO_PUBLIC_ENV": "testflight"
      }
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
